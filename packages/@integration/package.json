  "bugs": {
    "url": "https://github.com/marblejs/marble/issues"
  },
  "homepage": "https://github.com/marblejs/marble#readme",
  "dependencies": {
    "@marblejs/core": "^3.0.0-rc.3",
    "@marblejs/middleware-body": "^3.0.0-rc.3",
    "@marblejs/middleware-cors": "^3.0.0-rc.3",
    "@marblejs/middleware-io": "^3.0.0-rc.3",
    "@marblejs/middleware-logger": "^3.0.0-rc.3",
    "@marblejs/middleware-multipart": "^3.0.0-rc.3",
    "@marblejs/websockets": "^3.0.0-rc.3",
    "@types/node": "^10.1.2",
    "rxjs": "^6.5.3",
    "typescript": "~3.0.3"
