all: routing middlewares

routing:
	@./run 5 $@
	@./run 15 $@
	@./run 25 $@
	@./run 50 $@
	@./run 75 $@
	@./run 100 $@
	@./run 150 $@
	@./run 200 $@
	@echo

middlewares:
	@./run 5 $@
	@./run 15 $@
	@./run 25 $@
	@./run 50 $@
	@./run 75 $@
	@./run 100 $@
	@./run 150 $@
	@./run 200 $@
	@echo

.PHONY: all
